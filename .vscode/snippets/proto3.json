{
  "rpc": {
    "prefix": "rpc",
    "body": [
      "rpc Create${1:Resource}(Create${1:Resource}Request) returns (${1:Resource}){ option (google.api.http) ={ post: \"/v1/${3|s,n,u|}/${2:resources}\", body: \"*\" }; }",
      "rpc Get${1:Resource}(Get${1:Resource}Request) returns (${1:Resource}){ option (google.api.http).get =\"/v1/${3|s,n,u|}/${2:resources}/{id}\"; }",
      "rpc List${1:Resource}s(List${1:Resource}sRequest) returns (List${1:Resource}sResponse){ option (google.api.http).get =\"/v1/${3|s,n,u|}/${2:resources}\"; }",
      "rpc Update${1:Resource}(Update${1:Resource}Request) returns (${1:Resource}){ option (google.api.http) ={ post: \"/v1/${3|s,n,u|}/${2:resources}/{id}\", body: \"*\" }; }",
      "rpc Delete${1:Resource}(Delete${1:Resource}Request) returns (google.protobuf.Empty){ option (google.api.http).delete =\"/v1/${3|s,n,u|}/${2:resources}/{id}\"; }"
    ]
  },
  "msg": {
    "prefix": "msg",
    "body": [
      "message ${1:Resource} {",
      "\tstring id = 1;",
      "\tstring name = 2;",
      "\tstring namespace = 3;",
      "\tapi.type.Metadata metadata = 4;",
      "\tstring uri = 5;",
      "\tWriteOnce${1:Resource}Data wodata = 6;",
      "\tWriteMany${1:Resource}Data wdata = 7; ",
      "\tReadonly${1:Resource}Data rdata = 8;",
      "}",
      "message Readonly${1:Resource}Data {",
      "\t${3:// ReadonlyData}",
      "}",
      "message WriteMany${1:Resource}Data {",
      "\t${4:// WriteManyData}",
      "}",
      "message WriteOnce${1:Resource}Data {",
      "\t${5:// WriteOnceData}",
      "}",
      "message Create${1:Resource}Request {",
      "\tstring name = 1;",
      "\tstring namespace = 2;",
      "\tWriteOnce${1:Resource}Data wodata = 3;",
      "\tWriteMany${1:Resource}Data wdata = 4;",
      "}",
      "message Update${1:Resource}Request {",
      "\tstring id = 1;",
      "\tstring name = 2;",
      "\tstring namespace = 3;",
      "\tWriteMany${1:Resource}Data wdata = 5;",
      "}",
      "message Get${1:Resource}Request {",
      "\tstring id = 1;",
      "\tstring name = 2;",
      "\tstring namespace = 3;",
      "}",
      "message List${1:Resource}sRequest {",
      "\tstring name = 1;",
      "\tstring namespace = 2;",
      "\tstring filter = 3;",
      "\tstring order_by = 4;",
      "\tstring page_token = 5;",
      "\tint32 page_size = 6;",
      "}",
      "message List${1:Resource}sResponse {",
      "\trepeated ${1:Resource} ${2:resources} = 1;",
      "\tstring next_page_token = 2;",
      "\tint32 total_size = 3;",
      "}",
      "message Delete${1:Resource}Request {",
      "\tstring id = 1;",
      "\tstring name = 2;",
      "\tstring namespace = 3;",
      "}"
    ]
  }
}